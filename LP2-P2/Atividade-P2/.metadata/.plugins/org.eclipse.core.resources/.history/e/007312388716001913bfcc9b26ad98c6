package polimorfismo.questao3;

import java.util.List;

public class Subtracao extends Operacao{
	@Override
	public float calcular(float num1, float num2) {
		return num1 - num2; 
	}
	
	@Override
	public float calcular(List<Float> numeros) {
		float subtracao = 0; 
		
		for (Float numero : numeros) {
			subtracao -= numero;
		}
		
		return subtracao;
	}
}
